CREATE DATABASE furamar;
CREATE TABLE nhan_vien(
	ma_nhan_vien INT PRIMARY KEY,
	ten_nhan_vien VARCHAR(50),
	ngay_sinh_nhan_vien VARCHAR(50),
	so_CMND_nhan_vien INT,
	so_dien_thoai INT,
	luong_nhan_vien FLOAT,
	
	vi_tri INT,
	FOREIGN KEY (vi_tri) REFERENCES vi_tri_nhan_vien(stt_vi_tri_nhan_vien)
	
	trinh_do INT,
	FOREIGN KEY (trinh_do) REFERENCES trinh_do_nhan_vien(stt_trinh_do_nhan_vien)
	//da tri
	vi_tri
	trinh_do

);

CREATE TABLE vi_tri_nhan_vien(
	stt_vi_tri_nhan_vien INT PRIMARY KEY,
	ten_vi_tri_nhan_vien VARCHAR(50)
	

);

CREATE TABLE trinh_do_nhan_vien(
	stt_trinh_do_nhan_vien PRIMARY KEY,
	ten_trinh_do_nhan_vien VARCHAR(50)
	

);


CREATE TABLE hop_dong_thue(
	ma_hop_dong INT PRIMARY KEY,
	ten_khach_hang INT,
	dich_vu_su_dung
	ngay_bat_dau DATE,
	ngay_ket_thuc DATE,
	so_tien_coc_truoc FLOAT,
	tong_so_tien_thanh_toan FLOAT

	FOREIGN KEY (ten_khach_hang) REFERENCES 
);


CREATE TABLE dich_vu(
	ma_dich_vu INT PRIMARY KEY,
	ten_dich_vu VARCHAR (50),
	dien_tich_su_dung FLOAT,
	chi_phi_thue FLOAT,
	tieu_chuan_phong VARCHAR(50),
	mo_ta_tien_nghi VARCHAR (50),
	dien_tich_ho_boi FLOAT,
	dich_vu_mien_phi_di_kem VARCHAR(50),
	so_luong_nguoi_toi_da INT,
	kieu_thue INT,
	FOREIGN KEY (kieu_thue) REFERENCES kieu_thue(stt_kieu_thue)
	
	// da_tri
	kieu_thue

);


CREATE TABLE kieu_thue(
	stt_kieu_thue INT PRIMARY KEY,
	ten_kieu_thue VARCHAR(50)
		

);

CREATE TABLE khach_hang(
	ma_khach_hang INT PRIMARY KEY,
	ten_khach_hang VARCHAR(50),
	ngay_sinh DATE,
	gioi_tinh VARCHAR(50),
	so_CMND_khach_hang INT,
	so_DT_khach_hang INT,
	email_khach_hang VARCHAR(50),
	dia_chi VARCHAR(50),
	
	// da_tri
	loai_khach_hang INT,
	FOREIGN KEY (loai_khach_hang) REFERENCES loai_khach_hang(stt_loai_khach_hang)




);

CREATE TABLE loai_khach_hang(
	stt_loai_khach_hang INT PRIMARY KEY,
	ten_loai_khanh_hang VARCHAR(50),


);














